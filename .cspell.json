{
  "version": "0.1",
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json",
  "language": "en",
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_US",
    "npm",
    "misc",
    "filetypes"
  ],
  "words": [
    "autofix",
    "autofixers",
    "autofixes",
    "backticks",
    "bigint",
    "bitauth",
    "bitjson",
    "bivariant",
    "blockless",
    "cimg",
    "circleci",
    "codebases",
    "codecov",
    "Codecov",
    "commitlint",
    "contravariant",
    "declarators",
    "dependabot",
    "destructure",
    "destructured",
    "editorconfig",
    "emoji",
    "emojis",
    "errored",
    "erroring",
    "ESLint's",
    "ESLint",
    "esnext",
    "espree",
    "estree",
    "execa",
    "exponentiate",
    "globby",
    "IDE's",
    "IIFE",
    "IIFEs",
    "libauth",
    "linebreak",
    "linebreaks",
    "mkdir",
    "nocheck",
    "nullish",
    "OOM",
    "OOMs",
    "parameterised",
    "performant",
    "pluggable",
    "postprocess",
    "postprocessor",
    "Premade",
    "prettier's",
    "prettierignore",
    "recurse",
    "reimplement",
    "resync",
    "ROADMAP",
    "ruleset",
    "rulesets",
    "sandboxed",
    "shebang",
    "superset",
    "thenables",
    "transpiled",
    "transpiles",
    "tsconfigs",
    "tsutils",
    "typedef",
    "typedefs",
    "typedoc",
    "unary",
    "unfixable",
    "unprefixed",
    "untracked",
    "vars"
  ],
  "flagWords": [],
  "ignorePaths": [
    "package.json",
    "package-lock.json",
    "yarn.lock",
    "tsconfig.json",
    "node_modules/**",
    ".vscode/**",
    "**/coverage/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/fixtures/**",
    "**/**/CHANGELOG.md",
    "**/**/CONTRIBUTORS.md",
    "**/**/ROADMAP.md",
    "**/*.{json,snap}",
    ".cspell.json",
    "yarn.lock",
    ".github/workflows/**"
  ],
  "ignoreRegExpList": [
    "/```[\\w\\W]*?```/",
    "/~~~[\\w\\W]*?~~~/",
    "/``[\\w\\W]*?``/",
    "/`[^`]*`/",
    "/\\.\\/docs\\/rules\\/[^.]*.md/",
    "/@typescript-eslint\\/[a-z-]+/",
    "/\\.all-contributorsrc/",
    "/TS[^\\s]+/",
    "\\(#.+?\\)"
  ],
  "overrides": [
    {
      "filename": "**/*.{ts,js}",
      "ignoreRegExpList": ["/@[a-z]+/", "/#(end)?region/"],
      "includeRegExpList": [
        "/\\/\\*[\\s\\S]*?\\*\\/|([^\\\\:]|^)\\/\\/.*$/",
        "/(\\/\\/[^\\n\\r]*[\\n\\r]+)/"
      ]
    }
  ]
}
